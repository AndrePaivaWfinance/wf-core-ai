{
  "name": "wf-core-ai",
  "version": "1.0.0",
  "description": "MESH Platform - AI Financial Analyst Bot for Wfinance",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node src/index.js",
    "prod": "NODE_ENV=production node src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t wf-core-ai .",
    "docker:run": "docker run -p 3978:3978 wf-core-ai",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx audit-ci --moderate"
  },
  "keywords": [
    "botframework",
    "financial",
    "ai",
    "analyst",
    "azure",
    "restify",
    "wfinance"
  ],
  "author": "Wfinance Team",
  "license": "MIT",
  "dependencies": {
    "restify": "^11.1.0",
    "botbuilder": "^4.20.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "applicationinsights": "^2.8.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jest": "^29.6.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "audit-ci": "^6.0.0",
    "nodemon": "^3.0.1",
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrePaivaWfinance/wf-core-ai.git"
  },
  "bugs": {
    "url": "https://github.com/AndrePaivaWfinance/wf-core-ai/issues"
  },
  "homepage": "https://github.com/AndrePaivaWfinance/wf-core-ai#readme",
  "volta": {
    "node": "18.17.1",
    "npm": "9.6.7"
  }
}